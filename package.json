{
  "name": "yaml-go-to-definition",
  "version": "0.0.7",
  "engines": {
    "vscode": "^1.86.0"
  },
  "main": "./out/extension.js",
  "activationEvents": [
    "onLanguage:yaml"
  ],
  "scripts": {
    "vscode:prepublish": "npm run transpile",
    "transpile": "tsc -p ./",
    "transpile:watch": "tsc -watch -p ./",
    "package": "vsce package",
    "test:watch": "jest --watch",
    "deploy": "vsce publish"
  },
  "dependencies": {
    "glob": "10.3.10"
  },
  "devDependencies": {
    "@swc/core": "^1.3.107",
    "@swc/jest": "^0.2.34",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.16",
    "@types/vscode": "^1.86.0",
    "jest": "^29.7.0",
    "typescript": "^5.3.3"
  },
  "displayName": "yaml-go-to-definition",
  "description": "This is a simple extension that allows you to go to the definition of a whatever in a yaml file.",
  "publisher": "GolovinDaniil",
  "icon": "logo.png",
  "author": {
    "name": "Daniil Golovin",
    "email": "golovin.daniely@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DanielGolovin/yaml-go-to-definition"
  }
}
